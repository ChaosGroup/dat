{
	"esnext": true,
	"undef": true,
	"unused": true,
	"newcap": true,
	"noarg": true,
	"nonew": false,
	"strict": true,
	"globalstrict": true,
	"maxparams": 5,
	"maxdepth": 3,
	"maxstatements": 30,
	"maxlen": 120,
	"eqeqeq": true,
	"eqnull": true,
	"bitwise": true,
	"camelcase": true,
	"curly": true,
	"freeze": true,
	"immed": true,
	"indent": 2,
	"quotmark": "single",
	"latedef": false,
	"node": true,
	"proto": false,
	"expr": true,
	"noyield": true
}
